{
  "name": "umunandi.org",
  "description": "Umunandi is a UK based charity supporting orphans and vulnerable children in Zambia.",
  "homepage": "http://umunandi.org/",
  "authors": [
    {
      "name": "Mark Chitty",
      "email": "mark@umunandi.org"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.5",
    "composer/installers": "~1.0.12",
    "vlucas/phpdotenv": "^2.0.1",
    "johnpbloch/wordpress": "4.5.3",
    "oscarotero/env": "^1.0",
    "roots/wp-password-bcrypt": "1.0.0",
    "wpackagist-plugin/admin-collapse-subpages": "2.2",
    "wpackagist-plugin/advanced-custom-fields": "4.3",
    "wpackagist-plugin/ajax-thumbnail-rebuild": "1.12",
    "wpackagist-plugin/akismet": "3.0",
    "wpackagist-plugin/codepress-admin-columns": "2.2",
    "wpackagist-plugin/enable-media-replace": "2.9",
    "wpackagist-plugin/jc-submenu": "0.7",
    "wpackagist-plugin/pages-in-widgets": "1.7",
    "wpackagist-plugin/post-tags-and-categories-for-pages": "1.1",
    "wpackagist-plugin/redirection": "2.3.*",
    "wpackagist-plugin/save-with-keyboard": "2.1",
    "wpackagist-plugin/widget-css-classes": "1.4",
    "wpackagist-plugin/wordfence": "~5.1",
    "wpackagist-plugin/wp-mail-smtp": "0.9"
  },
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  }
}
