{
  "name": "umunandi.org",
  "description": "Umunandi is a UK based charity supporting orphans and vulnerable children in Zambia.",
  "homepage": "http://umunandi.org/",
  "authors": [
    {
      "name": "Mark Chitty",
      "email": "mark@umunandi.org"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "wp-sync-db/wp-sync-db",
        "type": "wordpress-plugin",
        "version": "1.5",
        "dist": {
          "type": "zip",
          "url": "https://github.com/wp-sync-db/wp-sync-db/archive/1.5.zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "wp-sync-db/wp-sync-db-media-files",
        "type": "wordpress-plugin",
        "version": "1.1.5",
        "dist": {
          "type": "zip",
          "url": "https://github.com/wp-sync-db/wp-sync-db-media-files/archive/master.zip"
        }
      }
    }
  ],
  "possible-repositories": [ {"type": "pear", "url": "https://pear.php.net"} ],
  "require": {
    "php": ">=7.0",
    "johnpbloch/wordpress": "*",
    "composer/installers": "*",
    "vlucas/phpdotenv": "*",
    "oscarotero/env": "*",
    "roots/wp-password-bcrypt": "*",
    "wp-sync-db/wp-sync-db": "*",
    "wp-sync-db/wp-sync-db-media-files": "*",
    "drewm/mailchimp-api": "*",
    "wpackagist-plugin/advanced-custom-fields": "*",
    "wpackagist-plugin/akismet": "*",
    "wpackagist-plugin/bootstrap-3-shortcodes": "*",
    "wpackagist-plugin/codepress-admin-columns": "*",
    "wpackagist-plugin/enable-media-replace": "*",
    "wpackagist-plugin/save-with-keyboard": "*",
    "wpackagist-plugin/widget-css-classes": "*",
    "wpackagist-plugin/wordfence": "*",
    "wpackagist-plugin/wp-mail-smtp": "*",
    "wpackagist-plugin/litespeed-cache": "*",
    "wpackagist-plugin/enable-svg-uploads": "*",
    "wpackagist-plugin/password-protected": "*",
    "wpackagist-plugin/admin-menu-editor": "*",
    "wpackagist-plugin/wp-nested-pages": "*",
    "wpackagist-plugin/widget-shortcode": "*",
    "wpackagist-plugin/wp-security-audit-log": "*"
  },
  "might-use-plugins": {
    "wpackagist-plugin/post-tags-and-categories-for-pages": "^1.4",
    "wpackagist-plugin/jc-submenu": "~0.8",
    "wpackagist-plugin/pages-in-widgets": "~1.7"
  },
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  }
}
